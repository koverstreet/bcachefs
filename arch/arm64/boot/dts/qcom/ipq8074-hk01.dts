#include "ipq8074.dtsi"

/ {
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	model = "Qualcomm Technologies, Inc. IPQ8074-HK01";
	compatible = "qcom,ipq8074-hk01", "qcom,ipq8074";
	interrupt-parent = <&intc>;

	aliases {
		serial0 = &blsp1_uart5;
		serial1 = &blsp1_uart3;
	};

	chosen {
		stdout-path = "serial0";
	};

	memory {
		device_type = "memory";
		reg = <0x0 0x40000000 0x0 0x20000000>;
	};
};

&blsp1_i2c2 {
	status = "ok";
};

&blsp1_spi1 {
	status = "ok";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;
	};
};

&blsp1_uart3 {
	status = "ok";
};

&blsp1_uart5 {
	status = "ok";
};

&pcie0 {
	status = "ok";
	perst-gpio = <&tlmm 61 0x1>;
};

&pcie1 {
	status = "ok";
	perst-gpio = <&tlmm 58 0x1>;
};

&pcie_phy0 {
	status = "ok";
};

&pcie_phy1 {
	status = "ok";
};

&qpic_bam {
	status = "ok";
};

&qpic_nand {
	status = "ok";

	nand@0 {
		reg = <0>;
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;
	};
};
